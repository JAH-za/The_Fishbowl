const BookingCategories = () => {
  const categories = ['Overnight', 'Work function', 'Weekend Away'];

  return (
    <section style={{ padding: '2rem' }}>
      <h1 style={{ fontFamily: 'MoglanDemo', fontSize: '3rem' }}>
        Choose Your Escape
      </h1>
      <h2 style={{ fontFamily: 'MoglanTypeface', fontSize: '1.5rem', marginBottom: '1rem' }}>
        Booking Categories
      </h2>
      <ul style={{ listStyle: 'none', padding: 0 }}>
        {categories.map((cat, index) => (
          <li key={index} style={{
            fontFamily: 'KugileRegular',
            fontSize: '1.2rem',
            marginBottom: '0.5rem',
            borderBottom: '1px solid #ccc',
            paddingBottom: '0.5rem'
          }}>
            {cat}
          </li>
        ))}
      </ul>
    </section>
  );
};

export default BookingCategories;
