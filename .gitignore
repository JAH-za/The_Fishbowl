import React from 'react';
import './GlobalStyles.css'; // Includes MoglanDemo, MoglanTypeface, KugileRegular

const OnboardingFlow = () => {
  return (
    <div style={{
      backgroundColor: '#fceeee',
      minHeight: '100vh',
      fontFamily: 'KugileRegular',
      display: 'flex',
      flexDirection: 'column',
      alignItems: 'center',
      justifyContent: 'center',
      padding: '3rem 2rem',
    }}>
      {/* Step 1: Welcome */}
      <h1 style={{
        fontFamily: 'MoglanDemo',
        fontSize: '3rem',
        color: '#8c3b4a',
        marginBottom: '1rem'
      }}>
        Welcome to The Fishbowl
      </h1>
      <p style={{
        fontFamily: 'MoglanTypeface',
        fontSize: '1.5rem',
        color: '#b2606d',
        textAlign: 'center',
        maxWidth: '500px'
      }}>
        Presence wrapped in intimacy. Choose how you want to feel.
      </p>

      {/* Step 2: Mood Selector */}
      <div style={{ margin: '3rem 0', width: '100%', maxWidth: '600px' }}>
        {['Intentional connection', 'Bold companionship', 'Serene escape'].map((mood, i) => (
          <button key={i} style={{
            display: 'block',
            width: '100%',
            padding: '1rem',
            marginBottom: '1rem',
            border: 'none',
            borderRadius: '10px',
            backgroundColor: '#ffe2e8',
            color: '#8c3b4a',
            fontSize: '1.2rem',
            fontFamily: 'KugileRegular',
