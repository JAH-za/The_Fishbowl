import React, { useState } from 'react';
import './GlobalStyles.css'; // Includes MoglanDemo, MoglanTypeface, KugileRegular

const FishbowlOnboarding = () => {
  const [identity, setIdentity] = useState('');
  const [escape, setEscape] = useState('');

  return (
    <main style={{
      backgroundColor: '#fceeee',
      minHeight: '100vh',
      fontFamily: 'KugileRegular',
      display: 'flex',
      flexDirection: 'column',
      alignItems: 'center',
      padding: '3rem 2rem',
    }}>
      {/* ðŸª· Step 1: Welcome */}
      <header style={{ textAlign: 'center', marginBottom: '2rem' }}>
        <img src="/assets/fishbowl-logo.png" alt="Fishbowl Logo" style={{ width: '100px' }} />
        <h1 style={{
          fontFamily: 'MoglanDemo',
          fontSize: '3rem',
          color: '#8c3b4a'
        }}>
          Welcome to The Fishbowl
        </h1>
        <p style={{
          fontFamily: 'MoglanTypeface',
          fontSize: '1.4rem',
          color: '#b2606d'
        }}>
          Where intimacy meets intention.
        </p>
      </header>

      {/* ðŸªª Step 2: Booking Identity */}
      <section style={{ marginBottom: '2rem', textAlign: 'center' }}>
        <h2 style={{
          fontFamily: 'MoglanTypeface',
          fontSize: '1.8rem',
          marginBottom: '1rem'
        }}>
          How would you like to enter?
        </h2>
        {['Seeker', 'Guest Companion', 'Host'].map((role, index) => (
          <button
            key={index}
            onClick={() => setIdentity(role)}
            style={{
              padding: '1rem',
              margin: '0.5rem',
              borderRadius: '10px',
              border: identity === role ? '2px solid #8c3b4a' : '1px solid #ccc',
              backgroundColor: identity === role ? '#ffe2e8' : '#fff',
              fontFamily: 'KugileRegular',
              cursor: 'pointer'
            }}
          >
            {role}
          </button>
        ))}
      </section>

      {/* ðŸŒ™ Step 3: Choose Your Escape */}
      <section style={{ textAlign: 'center' }}>
        <h2 style={{
          fontFamily: 'MoglanTypeface',
          fontSize: '1.8rem',
          marginBottom: '1rem'
        }}>
          Choose Your Escape
        </h2>
        {['Overnight', 'Work function', 'Weekend Away'].map((option, index) => (
          <button
            key={index}
            onClick={() => setEscape(option)}
            style={{
              padding: '1rem',
              margin: '0.5rem',
              borderRadius: '10px',
              border: escape === option ? '2px solid #8c3b4a' : '1px solid #ccc',
              backgroundColor: escape === option ? '#ffe2e8' : '#fff',
              fontFamily: 'KugileRegular',
              cursor: 'pointer'
            }}
          >
            {option}
          </button>
        ))}
      </section>

      {/* ðŸŽŸ Final CTA */}
      {identity && escape && (
        <button style={{
          marginTop: '3rem',
          padding: '1rem 2rem',
          backgroundColor: '#8c3b4a',
          color: '#fff',
          borderRadius: '20px',
          fontFamily: 'MoglanTypeface',
          fontSize: '1.2rem',
          border: 'none',
          cursor: 'pointer'
        }}>
          Slip Beneath the Surface
        </button>
      )}
    </main>
  );
};

export default FishbowlOnboarding;
